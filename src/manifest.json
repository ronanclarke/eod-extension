{
  "manifest_version": 2,

  "name": "EoD Browser",
  "description": "This gives you some shortcuts for WhatClinic EoD instances",
  "version": "0.5.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://jenkins.whatclinic.net/"
  ]
}
